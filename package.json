{
  "name": "@matthewpereira/prototype-comments",
  "version": "0.1.1-beta.0",
  "description": "A lightweight TypeScript library for prototype-friendly comment rendering.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build:lib": "vite build --config vite.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "build": "run-s build:lib build:demo",
    "test": "vitest run",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "comments",
    "prototype",
    "typescript",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/prototype-comments.git"
  },
  "bugs": {
    "url": "https://github.com/yourname/prototype-comments/issues"
  },
  "homepage": "https://github.com/yourname/prototype-comments#readme",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/react": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.8",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
